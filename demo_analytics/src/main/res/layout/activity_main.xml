<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.ROIQuerySDK.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/Theme.ROIQuerySDK.PopupOverlay" />

    </com.google.android.material.appbar.AppBarLayout>

    <Button
        android:id="@+id/button_analytics_core"
        android:layout_width="match_parent"
        android:textAllCaps="false"
        android:layout_margin="5dp"
        android:layout_height="wrap_content"
        android:text="Core"
        />
    <Button
        android:id="@+id/button_ad_report"
        android:layout_width="match_parent"
        android:textAllCaps="false"
        android:layout_margin="5dp"
        android:layout_height="wrap_content"
        android:text="Ad Report"
        />
    <Button
        android:id="@+id/button_iap_report"
        android:layout_width="match_parent"
        android:textAllCaps="false"
        android:layout_margin="5dp"
        android:layout_height="wrap_content"
        android:text="IAP_Report"
        />
     <Button
        android:id="@+id/button_ias_report"
        android:layout_width="match_parent"
        android:textAllCaps="false"
        android:layout_margin="5dp"
        android:layout_height="wrap_content"
        android:text="IAS_Report"
        />

    <Button
        android:id="@+id/button_test_memory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_margin="5dp"
        android:text="test"
        android:textAllCaps="false" />

</LinearLayout>