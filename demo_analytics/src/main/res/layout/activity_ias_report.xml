<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="?attr/actionBarSize"
        android:orientation="vertical"
        android:paddingStart="20dp"
        android:paddingTop="20dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/button_report_to_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="ias_to_show"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/button_track_show_success"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="ias_show_success"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/button_track_show_fail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="ias_show_fail"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/button_track_to_subscribe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="ias_to_subscribe"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/button_track_subscribe_success"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="ias_subscribe_success"
            android:textAllCaps="false" />

        <Button
            android:id="@+id/button_track_subscribe_fail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="2dp"
            android:text="ias_subscribe_fail"
            android:textAllCaps="false" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>