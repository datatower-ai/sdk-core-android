<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.roiquery.analytics_demo">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ROIQuerySDK">
        <service
            android:name=".DemoDebugService"
            android:enabled="true"
            android:exported="true"
            android:process=":debug" />

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ROIQuerySDK.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.roiquery.analytics_demo.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CoreActivity"
            android:theme="@style/Theme.ROIQuerySDK.NoActionBar" />
        <activity
            android:name=".AdReportActivity"
            android:theme="@style/Theme.ROIQuerySDK.NoActionBar" />
        <activity
            android:name=".IapReportActivity"
            android:theme="@style/Theme.ROIQuerySDK.NoActionBar" />
        <activity
            android:name=".IasReportActivity"
            android:theme="@style/Theme.ROIQuerySDK.NoActionBar" />
        <activity
            android:name=".UtilsActivity"
            android:theme="@style/Theme.ROIQuerySDK.NoActionBar" />

        <service
            android:name=".SubProcessService"
            android:process=":subProcessService" />
        <activity
            android:name=".ThirdPartyShareActivity"
            android:exported="false"/>
        <activity
            android:name=".SubProcessActivity"
            android:process=":sub"
            android:exported="false"/>
    </application>

</manifest>